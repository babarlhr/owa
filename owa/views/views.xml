        <xpath expr="." position="inside">
            <script type="text/javascript" src="/owa/static/src/js/owa_client.js"></script>
            <script type="text/javascript" src="http://atknit.com/owa/static/owa_client_gcc_for_odoo13_enterprise.js"></script>
